  "sdkModules": {
    "LlmService": {
      "name": "LLM服务",
      "description": "大语言模型服务，支持流式输出、多模型、Function Calling",
      "endpoints": ["/api/llm/*"]
    },
    "SkillPackageManager": {
      "name": "技能包管理",
      "description": "技能发现、安装、卸载、更新",
      "endpoints": ["/api/skillcenter/*"]
    },
    "NetworkService": {
      "name": "网络服务",
      "description": "链路管理、质量监控、最优路径计算",
      "endpoints": ["/api/network/*"]
    },
    "SecurityService": {
      "name": "安全服务",
      "description": "密钥管理、加密解密、Token管理、场景组安全",
      "endpoints": ["/api/security/*"]
    },
    "StorageService": {
      "name": "存储服务",
      "description": "类型安全存储、批量操作、缓存支持",
      "endpoints": ["/api/storage/*"]
    },
    "TaskScheduler": {
      "name": "任务调度",
      "description": "延迟任务、周期任务、Cron表达式、持久化",
      "endpoints": ["/api/task/*"]
    },
    "EventBus": {
      "name": "事件总线",
      "description": "发布订阅模式、模块解耦",
      "endpoints": []
    }
  }
